{
    "buildCommand": "next build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "functions": {
        "api/*.py": {
            "runtime": "python3.12",
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/py/:path*",
            "destination": "/api/:path*"
        }
    ]
}